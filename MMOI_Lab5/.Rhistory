df<-read.csv("Rainfall_1901_1975_PAK.csv", stringsAsFactors = TRUE)
data = ts(df$Rainfall, df$Year, frequency = 12, start = c(1901, 1), end = c(1975, 12))
plot(data, xlab = 'Year', ylab = 'Rainfall')
